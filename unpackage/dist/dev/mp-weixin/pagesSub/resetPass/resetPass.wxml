<view class="login"><view class="logo"><image class="logoImg margin" src="../../static/imags/logo2.jpg"></image></view><view class="mailbox"><input class="maliboxInput margin" type="text" placeholder="请输入新密码" data-event-opts="{{[['input',[['__set_model',['','newPassword','$event',[]]],['newPassHandel',['$event']]]]]}}" value="{{newPassword}}" bindinput="__e"/><block wx:if="{{newPassFlag}}"><text class="error margin">长度为8-16位字母+特殊符号</text></block><input class="maliboxInput margin" type="text" placeholder="请确认密码" data-event-opts="{{[['input',[['__set_model',['','confirmPassword','$event',[]]],['confirmPassHandel',['$event']]]]]}}" value="{{confirmPassword}}" bindinput="__e"/><block wx:if="{{confirmPassFlag}}"><text class="error margin">密码不对应</text></block></view><view class="send"><button data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="sendEali" bindtap="__e">去登陆</button></view></view>